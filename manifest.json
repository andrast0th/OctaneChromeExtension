{
  "name": "Octane Chrome Extension",
  "version": "1.0",
  "description": "Util for changing an entities phase in ALM Octane",
  "permissions": [
    "bookmarks", "cookies", "tabs", "http://*/*", "https://*/*", "activeTab", "<all_urls>"
  ],
  "browser_action": {
      "default_title": "Phases",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}